<!DOCTYPE html>
<html lang="en">
  <head>
    <title>Portfolio</title>
    <meta charset="utf-8">
    <meta name="description"  content="Crypto portfolio">
    <meta name="author"       content="Tundra Fizz">
    <meta name="viewport"     content="width=460">
    <link rel="shortcut icon" href="/img/icon.jpg">
    <link rel="stylesheet"    href="/css/wip.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.0.2/jquery.min.js"></script>
  </head>

  <body>
    <div class="content">
      <table>
        <tr>
          <th>Symbol</th>
          <th>Coin</th>
          <th style="min-width: 100px;">Current Price</th>
          <th>Volume</th>
          <th style="min-width: 80px;">Total Value</th>
        </tr>
        <% for(var i = 0; i < data.length; i++){ %>
          <tr>
            <td><%= data[i]["symbol"] %></td>
            <td><%= data[i]["name"] %></td>
            <td><%= data[i]["current_price"] %></td>
            <td><%= data[i]["volume"] %></td>
            <td><%= data[i]["total_value"] %></td>
          </tr>
        <% } %>
        <tr>
          <td colspan="4"></td>
          <td><%= grandTotal %></td>
        </tr>
      </table>
    </div>
  </body>
</html>
